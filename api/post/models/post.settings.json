{
  "connection": "default",
  "collectionName": "posts",
  "info": {
    "name": "post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "blog_categories": {
      "collection": "blog-categories"
    },
    "description": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "is_active": {
      "type": "boolean",
      "default": true
    }
  }
}
