{
  "connection": "default",
  "collectionName": "brands",
  "info": {
    "name": "product-brands"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "products": {
      "via": "brands",
      "collection": "product"
    }
  }
}
